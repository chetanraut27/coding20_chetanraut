import java.io.*;
public class Main
{
	public static void main(String[] args) 
	{
		int number = 987654, reverse = 0; 
		while(number!=0)
		{ 
		    int reminder = number%10; 
		    reverse = reverse*10+reminder; 
		    number = number/10; 
		} 
		System.out.println("The Reverse of given number is:"+reverse); 
	} 
}
